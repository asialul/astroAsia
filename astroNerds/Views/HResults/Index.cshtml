@model IEnumerable<astroNerds.Models.HResult>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HoroscopeType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoroscopeText)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ZodiacSign)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HoroscopeType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoroscopeText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ZodiacSign)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.HoroscopeID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.HoroscopeID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.HoroscopeID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
